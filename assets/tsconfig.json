{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom", "esnext.array"],
    "jsx": "react",
    "module": "ESNext",
    "moduleResolution": "node",
    "importHelpers": true,
    "removeComments": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": true
  }
}
